<?xml version="0.1"?>
<launch>

    <arg name="robot_name" default="alpha"/>

    <node pkg="libuvc_camera" type="camera_node" name="camera_node">
        <rosparam command="load" file="$(find alpha_config)/config/uvc_cam.yaml"/>
        <param name="frame_id" value="$(arg robot_name)/usb_cam"/>
    </node>
</launch>