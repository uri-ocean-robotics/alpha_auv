<?xml version="1.0"?>
<launch>

  <arg name="robot_name" default="alpha"/>

  <node pkg="gpsd_client" type="gpsd_client" name="gpsd_client" output="screen">
    <remap from="fix" to="gps/fix"/>
    <param name="frame_id" value="$(arg robot_name)/gps"/>
  </node>

  <node pkg="geographic_tools" type="geographic_tools_node" name="geographic_tools" output="screen">

  </node>

</launch>