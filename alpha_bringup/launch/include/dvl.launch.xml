<?xml version="1.0"?>
<launch>

  <arg name="robot_name" default="alpha"/>

  <node pkg="waterlinked_dvl" type="waterlinked_dvl_tcp_node" name="dvl_ros_node" output="screen">
    <rosparam command="load" file="$(find alpha_config)/config/dvl_a50.yaml"/>
    <param name="frame_id" value="$(arg robot_name)/dvl"/>
  </node>

</launch>

