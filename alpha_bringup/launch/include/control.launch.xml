<?xml version="1.0"?>
<launch>

    <arg name="robot_name" default="alpha"/>
    <arg name="robot_version" default="v1"/>

    <node pkg="mvp_control" type="mvp_control_ros_node" name="alpha_control" output="screen">
        <rosparam command="load" file="$(find alpha_config)/config/control_$(arg robot_version).yaml"/>
    </node>

</launch>