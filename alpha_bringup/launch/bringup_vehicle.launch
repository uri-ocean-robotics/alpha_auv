<?xml version="1.0"?>
<launch>

  <arg name="robot_name" default="alpha"/>

  <include file="$(find alpha_driver)/launch/driver.launch">
    <arg name="port" value="/dev/mbed_mcu"/>
  </include>

  <include file="$(find alpha_bringup)/launch/include/dvl.launch.xml">
    <arg name="robot_name" value="$(arg robot_name)"/>
  </include>

  <include file="$(find alpha_bringup)/launch/include/imu.launch.xml">
    <arg name="robot_name" value="$(arg robot_name)"/>
  </include>

</launch>
