<?xml version="1.0"?>
<launch>

  <arg name="joystick_configuration" default="logitech_f710"/>

  <node pkg="joy" type="joy_node" name="joy_node">
    <param name="autorepeat_rate" value="10"/>
    <param name="coalesce_interval" value="0.10"/>
  </node>


  <rosparam ns="teleop" file="$(find alpha_teleop)/config/controlled_$(arg joystick_configuration).yaml" command="load"/>
  <node pkg="joy_teleop" type="joy_teleop.py" name="joy_teleop"/>

</launch>
